def insertion_sort(A):
    n = len(A)
    for i in range(1, n):
        key = A[i]
        j = i - 1
        while j >= 0 and A[j] > key:
            A[j + 1] = A[j]
            j -= 1
        A[j + 1] = key

arr = [41, 68, 67, 10, 7, 69, 95, 43, 98]
print("Початковий масив:", arr)
insertion_sort(arr)
print("Відсортований масив:", arr)
